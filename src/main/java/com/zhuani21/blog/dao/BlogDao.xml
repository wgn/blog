<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhuani21.blog.dao.BlogDao" >

	<select id="getBlogList" parameterType="com.zhuani21.blog.auto.bean.Blog" resultType="com.zhuani21.blog.auto.bean.Blog">
		select * from blog where user_id=#{userId } and status=1 order by id desc limit 100;
	</select>
 
</mapper>